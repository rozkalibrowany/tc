	"macros": {
    "includeGuard": [
			"workbench.action.files.save",
			"extension.updateIncludeGuard",
			"workbench.action.files.save"
		],
	  "format_table": [
			"workbench.action.files.save",
			"extension.table.formatAll",
			"workbench.action.files.save"
		]
	},
	"editor.fontSize": 12,
	"editor.tabSize": 2,
	"debug.console.fontSize": 10,
	"terminal.integrated.fontSize": 10,
	"files.exclude": {
		"**/build": true,
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"C_Cpp.updateChannel": "Insiders",
	"workbench.editor.enablePreview": false,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"editor.minimap.enabled": false,
	"explorer.confirmDelete": false,
	"cmake-tools-helper.cmake_download_path": "/home/ks/.vscode/extensions/maddouri.cmake-tools-helper-0.2.1/cmake_download",
	"explorer.confirmDragAndDrop": false,
	"C_Cpp.loggingLevel": "Information",
	"update.mode": "manual",
	"extensions.autoUpdate": false,
	"editor.renderControlCharacters": true,
	"workbench.iconTheme": "eq-material-theme-icons",
	"vsicons.associations.folders": [{
			"icon": "src",
			"extensions": ["src"],
			"format": "svg",
			"overrides": "src"
		},
		{
			"icon": "src",
			"extensions": ["inc"],
			"format": "svg"
		},
		{
			"icon": "src",
			"extensions": ["graphs"],
			"format": "svg"
		}
	],
	"C_Cpp.default.includePath": [
		"/usr/include/",
		"${workspaceFolder}/**"
	],
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"[cpp]": {
		"editor.defaultFormatter": "xaver.clang-format"
	},
	"diffEditor.renderSideBySide": false,
	"python.languageServer": "Pylance",
	"markdown-table-formatter.markdownGrammarScopes": [
		"markdown",
		"restructuredtext"
	],
	"yaml.customTags": [
		"!encrypted/pkcs1-oaep scalar",
		"!vault scalar"
	],
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"window.zoomLevel": -1,
	"editor.accessibilitySupport": "off"
}